{
  "name": "student-loans",
  "engines": {
    "node": ">=6.9.5"
  },
  "version": "0.0.0",
  "description": "A tool for tracking and optimizing your student loans",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./config/webpack.development.js --progress",
    "build:production": "webpack --config ./config/webpack.production.js --progress",
    "build:stats": "webpack --config ./config/webpack.development.js --profile --json",
    "build:watch": "webpack --config ./config/webpack.development.js --progress --watch",
    "clean": "rimraf __build__",
    "deploy": "node ./scripts/deploy.js",
    "spec": "karma start --single-run --no-auto-watch config/karma.conf.js",
    "spec:watch": "karma start config/karma.conf.js",
    "start:watch": "node scripts/dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jneander/student-loans.git"
  },
  "author": "Jeremy Neander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jneander/student-loans/issues"
  },
  "homepage": "https://github.com/jneander/student-loans#readme",
  "devDependencies": {
    "@instructure/ui-core": "^4.0.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "instructure-icons": "^4.3.2",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.3.0",
    "prop-types": "^15.5.8",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "write-file-webpack-plugin": "^4.0.2"
  },
  "dependencies": {
    "@instructure/ui-themes": "^4.0.0",
    "@jneander/activity-routing": "^0.0.1",
    "firebase": "^4.0.0",
    "lodash": "^4.17.4",
    "normalize.css": "^6.0.0",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
