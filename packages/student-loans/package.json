{
  "name": "@jneander/author",
  "engines": {
    "node": ">=8.9.4"
  },
  "version": "0.0.1",
  "description": "A Writing Platform",

  "scripts": {
    "build": "NODE_ENV=development build-app --config ./config/app.js",
    "build:production": "NODE_ENV=production build-app --config ./config/app.js",
    "build:watch": "NODE_ENV=development build-app --config ./config/app.js --watch",
    "clean": "rimraf __build__",
    "deploy": "node ./scripts/deploy.js",
    "lint": "lint-code",
    "lint:fix": "lint-code --fix",
    "start:watch": "NODE_ENV=development build-app --config ./config/app.js --watch --serve",
    "test": "NODE_ENV=test run-specs --config config/specs.js",
    "test:watch": "NODE_ENV=test run-specs --config config/specs.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jneander/jlnio"
  },
  "author": "Jeremy Neander <jeremy.neander@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jneander/jlnio/issues"
  },
  "homepage": "https://github.com/jneander/jlnio",
  "dependencies": {
    "@instructure/ui-core": "^4.7.3",
    "@instructure/ui-themes": "^4.7.3",
    "create-react-context": "^0.2.1",
    "normalize.css": "^8.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  },
  "devDependencies": {
    "@jneander/dev-tools": "^0.0.5"
  },
  "private": true
}
